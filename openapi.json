    "components": {
        "schemas": {
            "BlackCard": {
                "title": "BlackCard",
                "required": [
                    "text",
                    "icon",
                    "deck",
                    "pick"
                ],
                "type": "object",
                "properties": {
                    "card_id": {
                        "title": "Card Id",
                        "type": "integer"
                    },
                    "text": {
                        "title": "Text",
                        "type": "string"
                    },
                    "icon": {
                        "title": "Icon",
                        "type": "string"
                    },
                    "deck": {
                        "title": "Deck",
                        "type": "string"
                    },
                    "pick": {
                        "title": "Pick",
                        "type": "integer"
                    }
                }
            },
            "CardsAgainstHumanityPlayer": {
                "title": "CardsAgainstHumanityPlayer",
                "required": [
                    "username",
                    "cards_in_hand"
                ],
                "type": "object",
                "properties": {
                    "username": {
                        "title": "Username",
                        "type": "string"
                    },
                    "cards_in_hand": {
                        "title": "Cards In Hand",
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/WhiteCard"
                        }
                    },
                    "points": {
                        "title": "Points",
                        "type": "integer",
                        "default": 0
                    },
                    "current_role": {
                        "title": "Current Role",
                        "type": "string",
                        "default": "PLAYER"
                    },
                    "submissions": {
                        "title": "Submissions",
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Submission"
                        }
                    }
                }
            },
            "DeckMetaData": {
                "title": "DeckMetaData",
                "required": [
                    "id_name",
                    "description",
                    "name",
                    "icon"
                ],
                "type": "object",
                "properties": {
                    "id_name": {
                        "title": "Id Name",
                        "type": "string"
                    },
                    "description": {
                        "title": "Description",
                        "type": "string"
                    },
                    "official": {
                        "title": "Official",
                        "type": "boolean",
                        "default": false
                    },
                    "name": {
                        "title": "Name",
                        "type": "string"
                    },
                    "icon": {
                        "title": "Icon",
                        "type": "string"
                    }
                }
            },
            "GamePreferences": {
                "title": "GamePreferences",
                "type": "object",
                "properties": {
                    "deck_preferences": {
                        "title": "Deck Preferences",
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/DeckMetaData"
                        }
                    },
                    "points_needed_for_win": {
                        "title": "Points Needed For Win",
                        "type": "integer",
                        "default": 10
                    },
                    "max_round_count": {
                        "title": "Max Round Count",
                        "type": "integer",
                        "default": 15
                    }
                }
            },
            "GameStateMachine": {
                "title": "GameStateMachine",
                "required": [
                    "room_name",
                    "black_cards",
                    "white_cards"
                ],
                "type": "object",
                "properties": {
                    "room_name": {
                        "title": "Room Name",
                        "type": "string"
                    },
                    "black_cards": {
                        "title": "Black Cards",
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/BlackCard"
                        }
                    },
                    "white_cards": {
                        "title": "White Cards",
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/WhiteCard"
                        }
                    },
                    "state": {
                        "title": "State",
                        "type": "string",
                        "default": "STARTING"
                    },
                    "last_winner": {
                        "$ref": "#/components/schemas/LastWinnerInfo"
                    },
                    "round_count": {
                        "title": "Round Count",
                        "type": "integer",
                        "default": 0
                    },
                    "currently_active_card": {
                        "$ref": "#/components/schemas/BlackCard"
                    },
                    "player_submissions": {
                        "title": "Player Submissions",
                        "type": "object",
                        "additionalProperties": {
                            "$ref": "#/components/schemas/Submission"
                        }
                    },
                    "player_lookup": {
                        "title": "Player Lookup",
                        "type": "object",
                        "additionalProperties": {
                            "$ref": "#/components/schemas/CardsAgainstHumanityPlayer"
                        }
                    }
                }
            },
            "GameStatePlayerView": {
                "title": "GameStatePlayerView",
                "required": [
                    "player",
                    "room_name",
                    "state",
                    "round_count",
                    "other_players"
                ],
                "type": "object",
                "properties": {
                    "player": {
                        "$ref": "#/components/schemas/CardsAgainstHumanityPlayer"
                    },
                    "room_name": {
                        "title": "Room Name",
                        "type": "string"
                    },
                    "state": {
                        "title": "State",
                        "type": "string"
                    },
                    "round_count": {
                        "title": "Round Count",
                        "type": "integer"
                    },
                    "currently_active_card": {
                        "$ref": "#/components/schemas/BlackCard"
                    },
                    "player_submissions": {
                        "title": "Player Submissions",
                        "type": "object",
                        "additionalProperties": {
                            "$ref": "#/components/schemas/Submission"
                        }
                    },
                    "other_players": {
                        "title": "Other Players",
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/PlayerOutsideView"
                        }
                    },
                    "last_winner": {
                        "$ref": "#/components/schemas/LastWinnerInfo"
                    }
                }
            },
            "HTTPValidationError": {
                "title": "HTTPValidationError",
                "type": "object",
                "properties": {
